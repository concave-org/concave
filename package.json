{
  "name": "@concave/concave",
  "version": "1.7.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Concave is a tiny JavaScript framework for building web applications.",
  "main": "dist/concave.cjs.js",
  "module": "dist/concave.esm.js",
  "unpkg": "dist/concave.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "release": "standard-version",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concave-org/concave.git"
  },
  "keywords": [
    "concave"
  ],
  "author": "Mats Pfeiffer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concave-org/concave/issues"
  },
  "homepage": "https://github.com/concave-org/concave#readme",
  "devDependencies": {
    "esm": "3.2.25",
    "jest": "24.8.0",
    "rollup": "1.16.3",
    "rollup-plugin-filesize": "6.1.1",
    "standard": "12.0.1",
    "standard-version": "6.0.1"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**",
      "!**/node_modules/**"
    ]
  }
}
